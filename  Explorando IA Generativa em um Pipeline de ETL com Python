    {
      "cell_type": "code",
      "source": [
        "for user in users:\n",
        "    if user['name'] == 'Arthur':\n",
        "        user['card']['limit'] = 79000000\n",
        "\n",
        "    if user['name'] == 'Laura':\n",
        "        user['card']['limit'] = 6545000\n",
        "\n",
        "    if user['name'] == 'Eduarda':\n",
        "        user['card']['limit'] = 3225500\n",
        "\n",
        "    if user['name'] == 'Celia':\n",
        "        user['card']['limit'] = 589999\n",
        "\n",
